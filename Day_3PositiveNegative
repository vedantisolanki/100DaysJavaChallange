class PositiveNagetive
{
       public static void main(String []args)
       {
           int a = -11;
            
        if(a>=0)
           
               {
                     System.out.println(a+"number is Positive");
                }
        else 
                  
                 {
                     System.out.println(a+"number is Negative");
                }
         }
}

